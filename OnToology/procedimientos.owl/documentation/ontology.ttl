esproc:contienePortal
      a       owl:ObjectProperty ;
      rdfs:comment "Portal que un fenómeno geográfico contiene (por ejemplo, una vía o un tramo de vía, aunque también puede ser un barrio, distrito, ciudad, etc.)"@es , <http://vocab.linkeddata.es/datosabiertos/def/urbanismo-infraestructuras/callejero> ;
      rdfs:label "contiene portal"@es ;
      rdfs:subPropertyOf geosparql:sfContains ;
      dcterms:issued "2014-03-28" .

esproc:tramoVia
      a       owl:ObjectProperty ;
      rdfs:comment "Tramo de vía al que pertenece un fenómeno (por ejemplo, una dirección, un portal, etc.)"@es ;
      rdfs:isDefinedBy <http://vocab.linkeddata.es/datosabiertos/def/urbanismo-infraestructuras/callejero> ;
      rdfs:label "pertenece al tramo de vía"@es ;
